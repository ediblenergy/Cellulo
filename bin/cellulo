#!/usr/bin/env perl
use strict;
use warnings;
use FindBin;
use lib "$FindBin::Bin/../lib";
use Games::Cellulo::Game;
use Devel::Dwarn;
my $g = Games::Cellulo::Game->new_with_options;

my $_all = sub {
    return @$_[0];
};

my $DESTROY = sub {
    warn 'DESTROY';
    warn "\n"x100000;
    exit;
};

$g->init;
$g->play;
